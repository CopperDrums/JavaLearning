public class Conditions_06 
{
    public static int a = 21;
    public static int b = 15;
    public static int sum;
    
    public static void main(String[] args) 
    {
        sum = a+b;
        
        if (sum > 40)
        {
            System.out.println(" > 40");
        }
        else
        {
            if (sum < 40 && sum > 30)
            {
                System.out.println("between 30 and 40");
            }
        }
    }
    
}
