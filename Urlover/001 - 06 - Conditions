
public class Github {
	
	public static void main(String[]arg) {
		
		//declare two integers
		int Num1 = 21;
		int Num2 = 15;
		
		//declare their sum
		int Sum = Num1 + Num2;
		
		if (Sum > 40) {
			System.out.print("It is bigger than 40 ");
		} else if (Sum < 40 && Sum > 30) {
			System.out.print("It is between 30 and 40");
		}
		
	} //end of main

} //end of class
