public class ex00103 {

    public static void main(String[] args) {
        String [] days = {"Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"};
        String [] phr = {"Today is ", "Tomorrow is ", ". ", "."};
        
        System.out.print(phr[0] + days[4] + phr[2]);
        System.out.print(phr[1] + days[5] + phr[3]);

    }
}
