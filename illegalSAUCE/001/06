public class ex00106 {

    public static void main(String [] args){
        int a = 21;
        int b = 15;
        int c = a + b;

        if (c > 40) {
            System.out.print("It is bigger than 40");
        
        } else if (c < 40 && c > 30) {
            System.out.print("It is between 30 and 40");
        
        }
    }
}
